a=int(input("enter the temprature:"))
if a<0 & a==0:
    print("freezing weather")
elif a>0 & a<=0:
    print("very cold weather")
elif a>10 & a<=20:
    print("cold weather")
elif a>20 & a<=30:
    print("normal temperature")
elif a>30 & a<=40:
    print("hot weather")
elif a>40:
    print("very hot")
